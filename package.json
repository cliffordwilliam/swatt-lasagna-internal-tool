{
  "name": "swatt-lasagna-internal-tool",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "delete": "dotenv -e .env.development.local -- node ./prisma/purge.js",
    "push": "dotenv -e .env.development.local -- npx prisma db push",
    "generate": "npx prisma generate",
    "seed": "dotenv -e .env.development.local -- npx prisma db seed",
    "update": "npm run delete && npm run push && npm run generate && npm run seed",
    "vercel-build": "npm run update && next build",
    "peek": "dotenv -e .env.development.local -- npx prisma studio"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "dependencies": {
    "@clerk/nextjs": "^5.7.2",
    "@prisma/client": "^5.20.0",
    "next": "14.2.15",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@types/node": "^20.16.11",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "dotenv-cli": "^7.4.2",
    "eslint": "^8",
    "eslint-config-next": "14.2.15",
    "postcss": "^8",
    "prisma": "^5.20.0",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3"
  }
}
