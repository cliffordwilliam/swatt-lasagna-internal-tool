{
  "name": "swatt-lasagna-internal-tool",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "vercel-build": "npx prisma generate && next build",
    "delete": "dotenv -e .env.development.local -- node ./prisma/purge.js",
    "push": "dotenv -e .env.development.local -- npx prisma db push",
    "generate": "npx prisma generate",
    "seed": "dotenv -e .env.development.local -- npx prisma db seed",
    "update": "npm run delete && npm run push && npm run generate && npm run seed",
    "peek": "dotenv -e .env.development.local -- npx prisma studio"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "dependencies": {
    "@clerk/elements": "^0.16.1",
    "@clerk/nextjs": "5.7.2",
